package models;

import play.db.jpa.Model;

public class VrstePlacanja extends Model {

	public int oznakaVrste;
	public String nazivVrstePlacanja;
	
	public VrstePlacanja() {
		// TODO Auto-generated constructor stub
	}

	public VrstePlacanja(int oznakaVrste, String nazivVrstePlacanja) {
		super();
		this.oznakaVrste = oznakaVrste;
		this.nazivVrstePlacanja = nazivVrstePlacanja;
	}

}
